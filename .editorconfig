root = true

[*]
indent_style = space
indent_size = 4
indent_size = unset
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.go]
indent_style = tab

[{src/go/tests/**,src/go/pt-mongodb-summary/test/**,src/go/pt-secure-collect/testdata/*}]
insert_final_newline = unset
[util/NaturalDocs/**]
trim_trailing_whitespace = unset

# TODO
[t/**]
insert_final_newline = unset
trim_trailing_whitespace = unset
